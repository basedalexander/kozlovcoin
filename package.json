{
  "name": "kozlovcoin",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "node1": "SERVER_PORT=3005 P2P_PORT=6000 npm start",
    "node2": "SERVER_PORT=3006 P2P_PORT=6001 P2P_PEERS=ws://localhost:6000 npm start",
    "dev": "nodemon",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && tsc -p .",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "compose": "docker-compose down && docker-compose build --no-cache && docker-compose up",
    "compose-up": "docker-compose up",
    "unit": "jest --config=jest.unit.config.js",
    "unit-watch": "jest --config=jest.unit.config.js --watch",
    "int": "jest --config=jest.int.config.js",
    "int-watch": "jest --config=jest.int.config.js --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "elliptic": "^6.4.0",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "mkdirp-promise": "^5.0.1",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jest": "^22.1.1",
    "@types/node": "^9.4.0",
    "@types/supertest": "^2.0.4",
    "jest-cli": "^22.1.4",
    "minimist": "^1.2.0",
    "nodemon": "^1.14.11",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.2",
    "typescript": "^2.7.1"
  }
